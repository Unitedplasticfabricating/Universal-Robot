    # calculate rxryrz
    m1 = get_rotation_matrix_from_description(&quot;x&quot;, -1, 135 * pi / 180) # rotation 1: 135 degrees forward about x axis
    m2 = get_rotation_matrix_from_description(&quot;z&quot;, 1, heading3 - pi/2)
